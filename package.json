{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite && node server",
    "build": "npm run build:client && npm run build:server",
    "preview": "vite preview",
    "start": "node server/index.js",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --ssr src/main.jsx --outDir dist/server",
    "generate": "vite build --outDir dist/static && npm run build:server && node prerender",
    "serve": "cross-env NODE_ENV=production node server",
    "debug": "node --inspect-brk server"
  },
  "dependencies": {
    "add": "^2.0.6",
    "axios": "^0.26.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "googleapis": "^100.0.0",
    "node-fetch": "^3.2.3",
    "node-postgres": "^0.6.2",
    "node-schedule": "^2.1.0",
    "nodemailer": "^6.7.3",
    "nodemon": "^2.0.15",
    "pg": "^8.7.3",
    "pg-format": "^1.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-uuid": "^1.0.2",
    "uuid": "^8.3.2",
    "uuidv4": "^6.2.13",
    "yarn": "^1.22.18"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^1.0.7",
    "autoprefixer": "^10.4.4",
    "postcss": "^8.4.12",
    "tailwindcss": "^3.0.24",
    "vite": "^2.9.0"
  }
}
